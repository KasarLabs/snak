{
  "name": "cairocoder-mcp-server",
  "version": "0.1.0",
  "description": "MCP server for generating, fixing, and managing Cairo code projects with AI assistance.",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "cairocoder-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cairo",
    "starknet",
    "code-generation",
    "ai",
    "llm",
    "blockchain"
  ],
  "author": "Kasar Labs",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "sqlite3": "^5.1.6",
    "axios": "^1.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.11",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasarlabs/cairocoder-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/kasarlabs/cairocoder-mcp-server/issues"
  },
  "homepage": "https://github.com/kasarlabs/cairocoder-mcp-server#readme"
}
