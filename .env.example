# Starknet configuration (mandatory)
STARKNET_PUBLIC_ADDRESS="YOUR_STARKNET_PUBLIC_ADDRESS"
STARKNET_PRIVATE_KEY="YOUR_STARKNET_PRIVATE_KEY"
STARKNET_RPC_URL="YOUR_STARKNET_RPC_URL"

# AI Provider configuration (mandatory)
AI_PROVIDER_API_KEY="YOUR_AI_PROVIDER_API_KEY"
AI_MODEL="YOUR_AI_MODEL"
AI_PROVIDER="YOUR_AI_PROVIDER"

# NestJS server configuration
SERVER_API_KEY="YOUR_SERVER_API_KEY"
SERVER_PORT="YOUR_SERVER_PORT"

# NODE_ENV
NODE_ENV?="YOUR_ENV_MODE" # development || production by default


# Agent additional configuration

# Discord Configuration
DISCORD_BOT_TOKEN?="YOUR_DISCORD_BOT_TOKEN"
DISCORD_CHANNEL_ID?="YOUR_DISCORD_CHANNEL_ID"

# Atlantic Credential
ATLANTIC_API_KEY?="YOUR_ATLANTIC_API_KEY"

# Upload large file
SECRET_PHRASE?="SECRET_PHRASE"
PATH_UPLOAD_DIR?="PATH_TO_UPLOAD_FILE"


# CairoCoder configuration
CAIRO_GENERATION_API_URL?="YOUR_CAIRO_GENERATION_API_URL" # API endpoint for generating Cairo code
CAIRO_UPLOAD_DIR?="PATH TO UPLOAD DIR" # Directory for CairoCoder to find Cairo files


#POSTGRES
POSTGRES_USER="YOUR_POSTGRES_USER"
POSTGRES_PASSWORD="YOUR_POSTGRES_PASSWORD"
POSTGRES_ROOT_DB="YOUR_POSTGRES_ROOT_DB"
POSTGRES_HOST="YOUR_POSTGRES_HOST"
POSTGRES_PORT="YOUR_POSTGRES_PORT"


