{
  "name": "Starknet RPC Agent",
  "interval": 15000,
  "chat_id": "ethereum_rpc",
  "autonomous": true,
  "plugins": [
    "rpc"
  ],
  "memory": true,
  "mcpServers": {
    "mcp-twikit": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/adhikasp/mcp-twikit",
        "mcp-twikit"
      ],
      "env": {
        "TWITTER_USERNAME": "${TWITTER_USERNAME}",
        "TWITTER_PASSWORD": "${TWITTER_PASSWORD}",
        "TWITTER_EMAIL": "${TWITTER_EMAIL}"
      }
    },
    "mcp-discord": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/hanweg/mcp-discord",
        "mcp-discord"
      ],
      "env": {
        "DISCORD_TOKEN": "${DISCORD_TOKEN}",
        "DISCORD_CHANNEL_ID": "${DISCORD_CHANNEL_ID}"
      }
    }
  }
}
