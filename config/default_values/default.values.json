{
    "global": {
        "base": 10
    },
    "agent": {
        "max_iterations": 10,
        "interval": 5,
        "mode": "interactive",
        "memory": {
            "enabled": false,
            "memory_size": 20,
            "short_term_memory_size": 5
        },
        "rag": {
            "enabled": false
        },
        "mcpServers": {}
    },
    "execution": {
        "model_default_timeout": 45000,
        "model_extended_timeout": 120000,
        "model_validation_timeout": 15000,
        "summarization_threshold": 2000,
        "default_stm_size": 5,
        "default_ltm_size": 20,
        "interactive_model": "gpt-4o-mini",
        "temperature": 0.1,
        "chars_per_token": 4,
        "word_token_divisor": 2,
        "max_validation_retry": 3,
        "default_max_graph_steps": 100,
        "max_content_preview_size": 150,
        "max_description_length": 300,
        "max_step_description_length": 300
    },
    "rag": {
        "embedding_model": "Xenova/all-MiniLM-L6-v2",
        "similarity_threshold": 0.5,
        "top_k": 4
    },
    "workers": {
        "file_ingestion": {
            "mutex_timeout": 300000,
            "retry_delay": 100,
            "max_retries": 50,
            "max_chunks": 200,
            "max_size_per_agent": 10000000,
            "max_size_per_user": 50000000,
            "chunks_sizes": [1000, 500, 200, 100],
            "chunks_overlap_multiplier": 0.1
        }
    }   
    
}